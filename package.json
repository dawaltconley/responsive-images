{
  "name": "@dawaltconley/responsive-images",
  "version": "0.4.0",
  "description": "Scripts and utilities for working with responsive images.",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./eleventy": "./dist/plugins/eleventy.js",
    "./sass": "./dist/sass/index.js",
    "./devices": "./dist/data/devices.js"
  },
  "files": [
    "_index.scss",
    "dist/**/*"
  ],
  "scripts": {
    "compile": "npx tsc",
    "build": "./build.sh",
    "lint": "npx eslint . && npx prettier -c .",
    "test": "npx vitest run && npm run lint",
    "test:watch": "npx vitest",
    "document": "npx typedoc src/index.ts src/sass/index.ts src/plugins/*.ts src/data/devices.ts",
    "prepublishOnly": "npm test && npm run build && npm run document",
    "prepack": "npm test && npm run build"
  },
  "keywords": [
    "images",
    "responsive",
    "eleventy",
    "eleventy-plugin"
  ],
  "author": "Dylan Awalt-Conley",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy-fetch": "^4.0.1",
    "@11ty/eleventy-img": "^4.0.2",
    "@types/11ty__eleventy-img": "^2.0.5",
    "@types/flat-cache": "^2.0.2",
    "@types/hast": "^3.0.4",
    "@types/lodash.groupby": "^4.6.9",
    "@types/node-fetch": "^2.6.11",
    "liquid-args": "^1.1.0",
    "lodash.groupby": "^4.6.0",
    "media-query-parser": "^2.0.2",
    "sass-cast": "^0.5.6"
  },
  "optionalDependencies": {
    "sass": "^1.45.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@types/lodash": "^4.17.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "hast-util-to-html": "^9.0.0",
    "liquidjs": "^10.10.1",
    "lodash": "^4.17.21",
    "node-html-parser": "^6.1.12",
    "prettier": "^2.8.8",
    "sass": "^1.71.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-scss": "^4.7.0",
    "typedoc": "^0.25.12",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  }
}
